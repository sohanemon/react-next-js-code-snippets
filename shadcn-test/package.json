{
  "name": "shadcn-anatomy",
  "displayName": "Shadcn Anatomy",
  "description": "Shadcn Anatomy code snippets!",
  "publisher": "sohanemon",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/sohanemon/vscode-extensions/tree/main/shadcn-anatomy"
  },
  "icon": "src/assets/icon.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "main": "out/extension",
  "categories": [
    "Snippets",
    "Other"
  ],
  "license": "LICENSE.md",
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "build": "bun src/bundler.js",
    "package": "npm run build && npx vsce package",
    "export": "npm run build && npx vsce publish"
  },
  "contributes": {
    "snippets": [
      {
        "language": "javascriptreact",
        "path": "dist/snippets.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "dist/snippets.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "shadcn.installComponents",
        "title": "Shadcn/Ui: Install components"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/vscode": "^1.85.0",
    "vscode": "^1.1.37"
  }
}
