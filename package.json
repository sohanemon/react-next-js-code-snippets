{
  "name": "react-next-js-code-snippets",
  "displayName": "React Next JS Code Snippets",
  "description": "React Next.js development with lightning-fast code snippets!",
  "publisher": "sohanemon",
  "repository": {
    "type": "git",
    "url": "https://github.com/sohanemon/react-next-js-code-snippets"
  },
  "icon": "src/assets/icon.png",
  "version": "0.2.14",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Snippets"
  ],
  "license": "LICENSE.md",
  "scripts": {
    "build": "bun src/bundler.js",
    "package": "npm run build && npx vsce package",
    "export": "npm run build && npx vsce publish"
  },
  "contributes": {
    "snippets": [
      {
        "language": "typescript",
        "path": "dist/snippets.code-snippets"
      },
      {
        "language": "javascript",
        "path": "dist/snippets.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "dist/snippets.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "dist/snippets.code-snippets"
      }
    ]
  },
  "dependencies": {
    "chalk": "^5.3.0"
  }
}
